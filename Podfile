use_frameworks!

pod "SwiftSparql"
pod "STRegex"
pod "Nuke"

abstract_target 'iOS' do
  platform :ios, '13.5'
  target 'App iOS'
  target 'Widget iOS'
  target 'Backend iOS'
end

abstract_target 'macOS' do
  platform :osx, '11.0'
  target 'App macOS'
  target 'Widget macOS'
  target 'Backend macOS'
end
